{
  "name": "shaarlimaterial",
  "version": "0.12.1",
  "description": "Shaarli Material is a theme for [Shaarli](https://github.com/shaarli/Shaarli), the personal, minimalist, super-fast, database free, bookmarking service.",
  "main": "src/js/main.js",
  "dependencies": {
    "@babel/runtime": "^7.22.11",
    "@babel/runtime-corejs3": "^7.22.11",
    "autosize": "^4.0.4",
    "awesomplete": "^1.1.5",
    "blazy": ">=1.3.0",
    "bootstrap": "^3.4.1",
    "core-js": "^3.32.1",
    "jquery": "^3.7.0",
    "qrcode": "1.4.4",
    "regenerator-runtime": "^0.13.11",
    "salvattore": "^1.0.9",
    "sortablejs": "^1.15.0"
  },
  "devDependencies": {
    "@babel/core": "^7.22.11",
    "@babel/plugin-transform-runtime": "^7.22.10",
    "@babel/preset-env": "^7.22.10",
    "@fullhuman/postcss-purgecss": "^2.3.0",
    "@rollup/plugin-babel": "^5.3.1",
    "@rollup/plugin-commonjs": "^17.1.0",
    "@rollup/plugin-eslint": "^8.0.5",
    "@rollup/plugin-node-resolve": "^11.2.1",
    "autoprefixer": "^9.8.8",
    "babel-eslint": "^10.1.0",
    "cross-env": "^7.0.3",
    "eslint": "^7.32.0",
    "eslint-config-semistandard": "^15.0.1",
    "eslint-config-standard": "^16.0.3",
    "eslint-plugin-import": "^2.28.1",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^4.3.1",
    "eslint-plugin-standard": "^5.0.0",
    "postcss": "^8.4.28",
    "postcss-clean": "^1.2.2",
    "purgecss": "^3.1.3",
    "rollup": "^2.79.1",
    "rollup-plugin-babel": "^4.4.0",
    "rollup-plugin-copy": "^3.4.0",
    "rollup-plugin-postcss": "^4.0.2",
    "rollup-plugin-terser": "^7.0.2",
    "sass": "^1.66.1"
  },
  "scripts": {
    "build": "cross-env NODE_ENV=production rollup -c",
    "dev": "cross-env NODE_ENV=development rollup -c --watch",
    "lint": "eslint src/js",
    "lint:fix": "eslint src/js --fix"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "browser": true,
      "es6": true
    },
    "extends": [
      "semistandard"
    ],
    "parserOptions": {
      "ecmaVersion": 2021,
      "parser": "babel-eslint"
    },
    "rules": {
      "node/no-callback-literal": "off",
      "node/handle-callback-err": "off"
    },
    "globals": {
      "shaarli": true
    }
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/kalvn/Shaarli-Material.git"
  },
  "keywords": [
    "shaarli",
    "material",
    "design"
  ],
  "author": "kalvn",
  "license": "MIT",
  "browserslist": [
    "last 5 versions",
    "ie >= 9"
  ]
}
